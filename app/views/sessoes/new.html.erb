<%= form_for :sessao, url: login_path do |f| %>
    <div class="field">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true %>
    </div>

    <div class="field">
      <%= f.label :senha %><br />
      <%= f.password_field :password, autocomplete: "off" %>
    </div>
    <div class="actions">
      <%= f.submit "Log in" %>
    </div>
    <p>Cliente novo? <%= link_to "Cadastre-se!", new_cliente_path %></p>
<% end %>